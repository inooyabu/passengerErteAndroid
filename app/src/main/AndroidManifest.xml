<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pemesanerte">

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".activity.EditPesananActivity"
            android:theme="@style/AppThemeActionBar"/>
        <activity
            android:name=".activity.CreateMultipleActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity android:name=".activity.example.CricketersActivity" />
        <activity android:name=".activity.example.DynamicViewsActivity" />
        <activity
            android:name=".activity.EditActivity"
            android:parentActivityName=".activity.AccountActivity"
            android:theme="@style/AppThemeActionBar1" />
        <activity
            android:name=".activity.RegisterActivity"
            android:parentActivityName=".activity.LoginActivity"
            android:theme="@style/AppThemeActionBar1" />
        <activity android:name=".activity.MyOrderActivity" />
        <activity
            android:name=".activity.example.CreateOrderActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity
            android:name=".activity.SelectTripActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity
            android:name=".activity.DetailOrderActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity android:name=".activity.AccountActivity" /> <!-- <activity android:name=".activity.KacauMyOrderActivity" /> -->
        <!-- <activity -->
        <!-- android:name=".activity.KacauRegisterActivity" -->
        <!-- android:parentActivityName=".activity.LoginActivity" -->
        <!-- android:theme="@style/AppThemeActionBar1" /> &lt;!&ndash;  &ndash;&gt; -->
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>