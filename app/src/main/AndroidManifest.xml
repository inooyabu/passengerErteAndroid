<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pemesanerte">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.MyOrderActivity"></activity>
        <activity
            android:name=".activity.CreateOrderActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity
            android:name=".activity.SelectTripActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity
            android:name=".activity.DetailOrderActivity"
            android:theme="@style/AppThemeActionBar" />
        <activity android:name=".activity.AccountActivity" />
        <activity android:name=".activity.KacauMyOrderActivity" />
        <activity android:name=".activity.RegisterActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>
    <uses-permission android:name="android.permission.INTERNET" />

    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

</manifest>